<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.desmoline.testforpregnancy.Developers">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:id="@+id/tableRow"
        android:weightSum="1"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <TextView
            android:layout_width="170dp"
            android:height="40dp"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="NAMES"
            android:id="@+id/textView3" />

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="MATRICULES"
            android:textAlignment="center"
            android:id="@+id/textView4"
            android:layout_alignBottom="@+id/tableRow"
            android:layout_alignRight="@+id/tableRow"
            android:layout_alignEnd="@+id/tableRow"

            />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:id="@+id/tableRow2"
        android:weightSum="1"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tableRow"
        android:layout_alignParentLeft="true">

        <TextView
            android:layout_height="wrap_content"
            android:text="Awa Melvine Khan"
            android:id="@+id/awa"
            style="@style/developers_text"/>

        <TextView
            android:layout_width="119dp"
            android:layout_height="40dp"
            android:text="FE13A026"
            android:textAlignment="center"
            android:id="@+id/textView6"
            android:layout_alignBottom="@+id/tableRow"
            android:layout_alignRight="@+id/tableRow"
            android:layout_alignEnd="@+id/tableRow"
            style="@style/developers_text"

            />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:id="@+id/tableRow3"
        android:weightSum="1"
        android:layout_below="@+id/tableRow2"
        android:layout_alignParentLeft="true">

        <TextView
            android:layout_height="wrap_content"
            android:text="Akumbom Marietta Ngwe"
            android:id="@+id/mariet"
            style="@style/developers_text"/>

        <TextView
            android:layout_width="119dp"
            android:layout_height="40dp"
            android:text="FE13A009"
            android:textAlignment="center"
            android:id="@+id/textView7"
            android:layout_alignBottom="@+id/tableRow"
            android:layout_alignRight="@+id/tableRow"
            android:layout_alignEnd="@+id/tableRow"
            android:layout_marginLeft="0dp"
            style="@style/developers_text"/>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:id="@+id/tableRow4"
        android:weightSum="1"
        android:layout_below="@+id/tableRow3"
        android:layout_alignParentLeft="true">

        <TextView
            android:layout_height="wrap_content"
            android:text="Bill Ngoh Acha"
            android:textSize="15sp"
            android:id="@+id/bill"
            style="@style/developers_text"/>

        <TextView
            android:layout_width="119dp"
            android:layout_height="40dp"
            android:text="FE13A048"
            android:textAlignment="center"
            android:id="@+id/textView8"
            android:layout_alignBottom="@+id/tableRow"
            android:layout_alignRight="@+id/tableRow"
            android:layout_alignEnd="@+id/tableRow"
            android:layout_marginLeft="0dp"

            style="@style/developers_text"/>
    </TableRow>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Developers"
        android:textSize="30sp"
        android:textColor="#4a7eaf"
        android:textAlignment="center"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/developers"
        android:textSize="18sp"
        android:padding="5dp"
        android:gravity="center_horizontal"
        android:id="@+id/textView9"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
